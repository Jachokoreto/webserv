{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file with make",
            "command": "/usr/bin/make",
            "args": [
                "r"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
